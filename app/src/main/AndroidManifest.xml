<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.Videira">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activitys.EditeMembro"></activity>
        <activity android:name=".Activitys.ListaAdapiter" />
        <activity
            android:name=".Activitys.Membros"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Activitys.Cadastro"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Activitys.Tela_Principal"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Activitys.ListaMenbros"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Activitys.Login"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>